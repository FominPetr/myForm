# Конец редиректа

#загрузка страниц без расширения
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php

#устоновка кодировки
AddDefaultCharset UTF-8